<config>
	<table
		name="Teams">
		<column
			name="Team_Number"
			type="int"
			text="Team Number"/>
		<column
			name="Team_Name"
			type="String"
			text="Team Name"/>
		<column
			name="High_School"
			type="String"
			text="High School"/>

	</table>

	<table
		name="Matches">
		<column
			name="Team_Number"
			type="int"
			text="Team Number"/>
		<column
			name="Match_Number"
			type="int"
			text="Match Number"/>
		<column
			name="Event_Name"
			type="String"
			text="Event Name"/>
		<column
			name="Red_One"
			type="int"
			text="Red One"/>
		<column
			name="Red_Two"
			type="int"
			text="Red Two"/>
		<column
			name="Red_Three"
			type="int"
			text="Red Three"/>
		<column
			name="Blue_One"
			type="int"
			text="Blue One"/>
		<column
			name="Blue_Two"
			type="int"
			text="Blue Two"/>
		<column
			name="Blue_Three"
			type="int"
			text="Blue Three"/>
	</table>

	<table
		name="Performance">
		<column
			name="Team_Number"
			type="Number"
			text="Team Number"/>
		<column
			name="Match_Number"
			type="Number"
			text="Match Number"/>
		<column
			name="Event_Name"
			type="String"
			text="Event Name"/>
		<column
			name="crossAutoLine"
			type="YorN"
			text="(Auto)Can the robot cross the auto line?"/>
		<column
			name="scoreSwitch"
			type="YorN"
			text="(Auto)Can it score on our switch?"/>
		<column
			name="scoreScale"
			type="YorN"
			text="(Auto)Can it score on a scale?"/>
		<column
			name="fouls"
			type="YorN"
			text="(Auto)Did they cause any fouls?"/>
		<column
			name="canSupport"
			type="YorN"
			text="(Tele-Op) Can the robot support other robots during climbing?"/>
		<column
			name="numCubes"
			type="Number"
			text="(Tele-Op)How many cubes did the robot pick up?"/>
		<column
			name="numPoints"
			type="Number"
			text="(Tele-Op) How many points did the robot score?"/>
		<column
			name="canClimb"
			type="YorN"
			text="(Tele-Op) Can the robot climb?"/>
		<column
			name="canPickCubes"
			type="YorN"
			text="(Tele-Op) Can the robot pick up cubes?"/>
		<column
			name="driver"
			type="String"
			text="(Tele-Op) How did the driver drive?"/>
	</table>

	<table
		name="Events">
		<column
			name="Event_Name"
			type="String"
			text="Event Name"/>
		<column
			name="Date"
			type="String"
			text="Date"/>
		<column
			name="Location"
			type="String"
			text="Location"/>
	</table>
</config>
